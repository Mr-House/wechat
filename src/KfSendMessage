package com.wechat.bean.kfSendMessage;

import java.io.Serializable;

public class KfSendMessage implements Serializable {
	
	/**
	 * 
	 */
	private static final long serialVersionUID = -8237547477753806468L;
	
	private String touser;
	private String msgtype;
	private KfSendMessageText text;
	
	public String getTouser() {
		return touser;
	}
	public void setTouser(String touser) {
		this.touser = touser;
	}
	public String getMsgtype() {
		return msgtype;
	}
	public void setMsgtype(String msgtype) {
		this.msgtype = msgtype;
	}
	public KfSendMessageText getText() {
		return text;
	}
	public void setText(KfSendMessageText text) {
		this.text = text;
	}
	
	
}
